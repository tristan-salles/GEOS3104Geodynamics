##Mantle 
	Material_0_reproduction=off	# allow tracer reproduction (default=on)
	Material_0_density=1e5		# density (default=1.0)
	Material_0_Bulk_visc=-1.0	# bulk visc ratio at initial porosity ( >1.0(2D), >2/3(3D) )
