	Sampling_lagrangian=0,1,1	# (fixed) Eulerian=0, Lagrangian=1 (default=0)
	Sampling_plot_num=0,0,0,0	# PPM file in which profile is stored (default=0)
	Sampling_x=0.1,0.5,0.9		# initial x location (default=0.0)
