	region_compression_z=0.75	# relative compression (0<compr<1)
	region_width_z=0.2		# (half)width of compressed range
	region_edge_width_z=0.05		# transition smoothing length

	mgunitx=3		# (coarsest) multigrid cell dimensions (elements)
	mgunitz=3		# (mgunitx/y/z default=min=2)
	mgunity=3		# (mgunitx/y/z default=min=2)
	levels=3		# number of multigrid levels (default=1)
	pmg_levels=1		# number of pressure multigrid levels (default=1)
