	Sampling_dirn=2,2,2,2		# profile direction (1=x, 2=z, 3=y) (default=0)
	Sampling_normalize=1,1,1	# 0=unnormalized, 1=normalized (default=0)
	Sampling_plot_min=0,0,0		# If not autoranging, then need a max/min for the scale
